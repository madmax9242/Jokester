<h1>Login to your profile</h1>


<% @users.each do |user| %>

<%= user.username %><br>

<strong><%= link_to 'Show', user %></strong> |
<strong><%= link_to 'Edit', edit_user_path(user) %></strong> |
<strong><%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure you want to delete this?  That is pure gold!' } %></strong><br>

<% end %>

<%= link_to 'Register', new_user_path %>
